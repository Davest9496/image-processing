{
  "name": "image-processing",
  "version": "1.0.0",
  "description": "Image resizing API",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
     "build": "tsc && npm run copy-static",
    "copy-static": "copyfiles -u 1 \"src/public/**/*\" dist/src/public && copyfiles -u 1 \"src/views/**/*\" dist/src/views",
    "postbuild": "npm run copy-static",
    "vercel-build": "npm run build",
    "dev": "nodemon src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "copyfiles": "^2.4.1",
    "jasmine": "^5.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
