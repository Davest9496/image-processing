    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jasmine": "^5.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "ts-node": "^10.9.2",
